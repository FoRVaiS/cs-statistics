  },
  "scripts": {
    "_TMPL:build": "npm run clean && npm run gen:gql && npm run _TMPL:build:tsc",
    "_TMPL:build:tsc": "tsc --project src/server/tsconfig.json",
    "_TMPL:start": "node -r dotenv/config .",
    "build:prod": "npm run _TMPL:build && webpack --config webpack.prod.js",
    "clean": "rimraf built/ dist/ src/__generated__/",
    "commit": "npm run lint && git-cz",
    "gen:gql": "graphql-codegen --config codegen.yml",
    "lint": "eslint --ext .ts --ext .tsx src --fix && stylelint src/client",
    "start": "npm run _TMPL:build && cross-env NODE_ENV=development npm run _TMPL:start",
    "start:prod": "npm run build:prod && cross-env NODE_ENV=production npm run _TMPL:start",
    "storybook": "cross-env IS_STORYBOOK=true start-storybook -c .storybook",
    "test": "jest",
    "test:watch": "jest --watchAll"
  }
